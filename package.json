{
  "name": "fabric-x",
  "version": "0.1.0",
  "description": "Structural react components library",
  "repository": "https://github.com/siuvdlec/fabric-x",
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "--lint": "manypkg check && eslint . --ext .ts,.tsx",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint-fix:manypkg": "manypkg fix",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier-fix": "prettier --write \"**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "predeploy-storybook": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "postdeploy-storybook": "rm -r storybook-static",
    "build-storybook": "build-storybook",
    "tsc": "tsc",
    "prebuild": "npm run tsc",
    "build": "preconstruct build",
    "preconstruct:init": "preconstruct init",
    "preconstruct:fix": "preconstruct fix"
  },
  "author": "Iacopo <siuvdlec@gmail.com>",
  "license": "MIT",
  "homepage": "http://siuvdlec.github.io/fabric-x",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/fabric-x.cjs.js",
  "module": "dist/fabric-x.esm.js",
  "files": [
    "/dist",
    "/components",
    "/css",
    "/providers",
    "/themes"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "components/index.ts",
      "css/common/index.ts",
      "providers/FabricProvider.tsx",
      "themes/std/index.ts"
    ]
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/fabric-x.esm.js",
      "default": "./dist/fabric-x.cjs.js"
    },
    "./components": {
      "module": "./components/dist/fabric-x-components.esm.js",
      "default": "./components/dist/fabric-x-components.cjs.js"
    },
    "./css/common": {
      "module": "./css/common/dist/fabric-x-css-common.esm.js",
      "default": "./css/common/dist/fabric-x-css-common.cjs.js"
    },
    "./providers/FabricProvider": {
      "module": "./providers/FabricProvider/dist/fabric-x-providers-FabricProvider.esm.js",
      "default": "./providers/FabricProvider/dist/fabric-x-providers-FabricProvider.cjs.js"
    },
    "./themes/std": {
      "module": "./themes/std/dist/fabric-x-themes-std.esm.js",
      "default": "./themes/std/dist/fabric-x-themes-std.cjs.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@manypkg/cli": "^0.19.1",
    "@preconstruct/cli": "^2.2.1",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vanilla-extract/babel-plugin": "^1.1.7",
    "@vanilla-extract/webpack-plugin": "^2.1.11",
    "babel-loader": "^8.2.5",
    "eslint": "^8.22.0",
    "eslint-config-facile": "^0.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-storybook": "^0.6.4",
    "fp-ts": "^2.12.2",
    "gh-pages": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "fp-ts": "^2.12",
    "react": "^18.2.0",
    "tslib": "^2.1.0"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.7.2",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/sprinkles": "^1.4.1",
    "clsx": "^1.2.1"
  }
}
